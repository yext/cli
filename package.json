  "devDependencies": {
    "@types/cli-spinner": "^0.2.3",
    "@types/fs-extra": "11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.9.1",
    "@types/prompts": "2.4.9",
    "@types/semver": "7.5.6",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.28.1",
    "execa": "8.0.1",
    "fs-extra": "11.2.0",
    "generate-license-file": "^3.0.0",
    "prettier": "^3.0.3",
