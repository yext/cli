{
  "name": "@yext/cli",
  "version": "1.0.0",
  "description": "Command-line Interface for the Yext Platform",
  "main": "dist/index.js",
  "author": "jwartofsky@yext.com",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    ".": "./dist/cli.js"
  },
  "types": "dist/cli.d.ts",
  "files": [
    "dist/**/*",
    "install.js",
    "THIRD_PARTY_NOTICES"
  ],
  "bin": {
    "yext": "dist/cli.js"
  },
  "engines": {
    "node": ">=17"
  },
  "scripts": {
    "prepack": "pnpm build",
    "postinstall": "node install",
    "build": "tsup",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite && node scripts/license_append.js",
    "fmt": "prettier --write --cache .",
    "lint": "eslint --cache --fix ."
  },
  "dependencies": {
    "bin-wrapper": "^4.1.0",
    "root-package-json": "^1.1.6",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.9.1",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.28.1",
    "generate-license-file": "^2.0.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.8.0",
  "yextCliVersion": "0.1_381"
}